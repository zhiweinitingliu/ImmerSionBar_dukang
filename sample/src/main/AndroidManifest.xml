<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gyf.immersionbar">

    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />
    <uses-permission android:name="android.permission.SYSTEM_OVERLAY_WINDOW" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.READ_LOGS" />


    <application
        android:name=".MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.MainActivity"
            android:configChanges="orientation|keyboardHidden|screenSize">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.PicAndColorActivity" />
        <activity
            android:name=".activity.PicActivity"
            android:theme="@style/AppTheme.Transparent" />
        <activity android:name=".activity.ColorActivity" />
        <activity
            android:name=".activity.BackActivity"
            android:theme="@style/AppTheme.Transparent" />
        <activity android:name=".activity.FragmentActivity" />
        <activity
            android:name=".activity.FragmentOneActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:windowSoftInputMode="adjustResize" />
        <activity
            android:name=".activity.FragmentTwoActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:windowSoftInputMode="adjustResize" />
        <activity
            android:name=".activity.FragmentThreeActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:windowSoftInputMode="adjustResize" />
        <activity
            android:name=".activity.FragmentFourActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:windowSoftInputMode="adjustResize" />
        <activity
            android:name=".activity.FragmentFiveActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:windowSoftInputMode="adjustResize" />
        <activity android:name=".activity.OverActivity" />
        <activity
            android:name=".activity.Over1Activity"
            android:windowSoftInputMode="adjustResize" />
        <activity
            android:name=".activity.Over2Activity"
            android:windowSoftInputMode="adjustResize" />
        <activity
            android:name=".activity.Over3Activity"
            android:windowSoftInputMode="adjustResize" />
        <activity
            android:name=".activity.Over4Activity"
            android:windowSoftInputMode="adjustResize" />
        <activity
            android:name=".activity.Over5Activity"
            android:windowSoftInputMode="adjustResize" />
        <activity
            android:name=".activity.Over6Activity"
            android:windowSoftInputMode="adjustResize" />
        <activity android:name=".activity.KeyBoardActivity" />
        <activity android:name=".activity.CoordinatorActivity" />
        <activity
            android:name=".activity.TabLayoutActivity"
            android:theme="@style/AppTheme.Transparent" />
        <activity android:name=".activity.FlymeActivity" />
        <activity android:name=".activity.ShapeActivity" />
        <activity
            android:name=".activity.ActionBarActivity"
            android:theme="@style/ActionBar"
            android:windowSoftInputMode="adjustResize" />
        <activity android:name=".activity.WhiteStatusBarActivity" />
        <activity android:name=".activity.WebActivity" />
        <activity
            android:name=".activity.DialogActivity"
            android:windowSoftInputMode="adjustResize" />
        <activity android:name=".activity.BlogActivity" />
    </application>

</manifest>